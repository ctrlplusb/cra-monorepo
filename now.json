{
  "version": 2,
  "name": "cra-monorepo",
  "builds": [
    { "src": "node/api/**/*.ts", "use": "@now/node" },
    {
      "src": "www/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/node/api/$1.ts"
    },
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/www/static/$1"
    },
    { "src": "/favicon.ico", "dest": "/www/favicon.ico" },
    {
      "src": "/asset-manifest.json",
      "dest": "/www/asset-manifest.json"
    },
    { "src": "/manifest.json", "dest": "/www/manifest.json" },
    {
      "src": "/precache-manifest.(.*)",
      "dest": "/www/precache-manifest.$1"
    },
    {
      "src": "/service-worker.js",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/www/service-worker.js"
    },
    {
      "src": "/sockjs-node/(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/www/sockjs-node/$1"
    },
    {
      "src": "/__webpack_dev_server__/(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/www/__webpack_dev_server__/$1"
    },
    {
      "src": "/(.*).hot-update.js.map",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/www/$1.hot-update.js.map"
    },
    {
      "src": "/(.*).hot-update.js",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/www/$1.hot-update.js"
    },
    {
      "src": "/(.*).hot-update.json",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/www/$1.hot-update.json"
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/www/index.html"
    }
  ]
}
